<documentation title="Compound Namespace Depth">
    <standard>
    <![CDATA[
    Compound namespaces with a depth of more than two MUST NOT be used.
    ]]>
    </standard>
    <code_comparison>
        <code title="Valid: Max depth of 2.">
        <![CDATA[
use _PhpScoperef5048aa2573\Vendor\Package\SomeNamespace\{
    _PhpScoperef5048aa2573\SubnamespaceOne\ClassA,
    _PhpScoperef5048aa2573\SubnamespaceOne\ClassB,
    _PhpScoperef5048aa2573\SubnamespaceTwo\ClassY,
    ClassZ,
};
        ]]>
        </code>
        <code title="Invalid: Max depth of 3.">
        <![CDATA[
use _PhpScoperef5048aa2573\Vendor\Package\SomeNamespace\{
    <em>_PhpScoperef5048aa2573\SubnamespaceOne\AnotherNamespace\ClassA,</em>
    _PhpScoperef5048aa2573\SubnamespaceOne\ClassB,
    ClassZ,
};
        ]]>
        </code>
    </code_comparison>
</documentation>
