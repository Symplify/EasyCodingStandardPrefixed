{
    "name": "symplify\/coding-standard",
    "description": "Set of Symplify rules for PHP_CodeSniffer and PHP CS Fixer.",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "nette\/utils": "^3.0",
        "nette\/finder": "^2.5",
        "nikic\/php-parser": "^4.10.2",
        "squizlabs\/php_codesniffer": "^3.5",
        "slevomat\/coding-standard": "^6.4",
        "friendsofphp\/php-cs-fixer": "^2.16",
        "phpstan\/phpstan": "^0.12.40",
        "symplify\/symplify-kernel": "dev-master",
        "symplify\/package-builder": "dev-master",
        "symplify\/autowire-array-parameter": "dev-master"
    },
    "require-dev": {
        "nette\/application": "^3.0",
        "nette\/bootstrap": "^3.0",
        "symplify\/easy-coding-standard-tester": "dev-master",
        "symplify\/package-builder": "dev-master",
        "symplify\/phpstan-extensions": "dev-master",
        "symfony\/http-kernel": "^4.4|^5.1",
        "doctrine\/orm": "^2.7",
        "symplify\/smart-file-system": "dev-master",
        "symfony\/framework-bundle": "^4.4|^5.1",
        "migrify\/php-config-printer": "^0.3.50",
        "phpunit\/phpunit": "^8.5|^9.4"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\CodingStandard\\": "src",
            "Symplify\\CodingStandard\\CognitiveComplexity\\": "packages\/cognitive-complexity\/src",
            "Symplify\\CodingStandard\\ObjectCalisthenics\\": "packages\/object-calisthenics\/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\CodingStandard\\Tests\\": "tests",
            "Symplify\\CodingStandard\\ObjectCalisthenics\\Tests\\": "packages\/object-calisthenics\/tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}